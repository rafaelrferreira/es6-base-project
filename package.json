{
  "name": "es6",
  "version": "1.0.0",
  "description": "ES6+ Project Reference",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "server": "http-server -o",
    "dev": "babel ./src/main.js -o ./public/bundle.js -w",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  },
  "keywords": [
    "es6",
    "babel",
    "template",
    "example"
  ],
  "author": "rafaelrferreira",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "i": "^0.3.7",
    "jest": "^28.0.3",
    "npm-check-updates": "^12.5.10"
  }
}
